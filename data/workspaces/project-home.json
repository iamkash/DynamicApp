        {
          "id": 2,
          "name": "Production Analytics Dashboard",
          "description": "Focuses on tracking oil and gas production rates, cumulative production, and production forecasts. Useful for assessing production trends and optimizing output."
        },
        {
          "id": 3,
          "name": "Well Performance Dashboard",
          "description": "Displays detailed performance indicators for each well, including production rates, downtime, and efficiency metrics. Helps in identifying underperforming wells and areas for improvement."
        },
        {
          "id": 4,
          "name": "Reservoir Pressure Monitoring Dashboard",
          "description": "Dedicated to monitoring reservoir pressure over time, highlighting trends and potential issues. Essential for maintaining reservoir integrity and optimizing injection strategies."
        },
        {
          "id": 5,
          "name": "Drilling Operations Dashboard",
          "description": "Tracks drilling activities, including drilling rates, mud usage, and equipment performance. Facilitates efficient drilling operations and resource management."
        },
        {
          "id": 6,
          "name": "Cumulative Production Overview Dashboard",
          "description": "Visualizes cumulative oil and gas production across different reservoirs or fields, aiding in long-term performance assessment and strategic planning."
        },
        {
          "id": 7,
          "name": "Reservoir Health Metrics Dashboard",
          "description": "Showcases key health indicators such as porosity, permeability, and fluid saturation. Crucial for evaluating reservoir conditions and planning interventions."
        },
        {
          "id": 8,
          "name": "Environmental Impact Dashboard",
          "description": "Monitors environmental metrics like CO₂ emissions, water usage, and waste generation. Supports sustainability initiatives and regulatory compliance."
        },
        {
          "id": 9,
          "name": "Maintenance and Reliability Dashboard",
          "description": "Lists upcoming and completed maintenance tasks, equipment reliability statistics, and maintenance costs. Ensures timely maintenance and reduces operational downtime."
        },
        {
          "id": 10,
          "name": "Reservoir Simulation Dashboard",
          "description": "Integrates simulation results to predict reservoir behavior under various scenarios. Assists in making informed decisions for reservoir management and development."
        },
        {
          "id": 11,
          "name": "Data Integrity and Quality Dashboard",
          "description": "Monitors the quality and integrity of data inputs, highlighting discrepancies or gaps. Ensures reliable data for accurate reservoir analysis and reporting."
        },
        {
          "id": 12,
          "name": "Operational Efficiency Dashboard",
          "description": "Analyzes operational workflows, resource allocation, and process efficiencies. Aims to streamline operations and enhance overall productivity."
        },
        {
          "id": 13,
          "name": "Risk Assessment Dashboard",
          "description": "Identifies and evaluates potential risks related to reservoir operations, including technical, environmental, and financial risks. Facilitates proactive risk management strategies."
        },
        {
          "id": 14,
          "name": "Economic Indicators Dashboard",
          "description": "Tracks economic metrics such as production costs, revenue, and profitability. Supports financial planning and investment decisions within reservoir projects."
        },
        {
          "id": 15,
          "name": "Hydraulic Fracturing Dashboard",
          "description": "Monitors hydraulic fracturing operations, including fracture propagation, fluid volumes, and pressure profiles. Essential for optimizing fracturing processes and enhancing reservoir connectivity."
        },
        {
          "id": 16,
          "name": "Gas Injection Monitoring Dashboard",
          "description": "Keeps track of gas injection rates, pressures, and volumes. Vital for maintaining pressure support and enhancing oil recovery in depleted reservoirs."
        },
        {
          "id": 17,
          "name": "Seismic Data Visualization Dashboard",
          "description": "Presents seismic survey data through interactive visualizations, aiding in subsurface mapping and reservoir characterization."
        },
        {
          "id": 18,
          "name": "Field Development Planning Dashboard",
          "description": "Assists in planning field development strategies, including well placement, infrastructure needs, and resource allocation."
        },
        {
          "id": 19,
          "name": "Asset Management Dashboard",
          "description": "Manages reservoir-related assets, tracking their status, utilization, and lifecycle. Ensures optimal asset performance and longevity."
        },
        {
