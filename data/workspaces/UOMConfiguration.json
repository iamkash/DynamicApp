{
  "id": "workspace1",
  "name": "Development Workspace",
  "path": "/dashboard",
  "icon": "FaChartPie",
  "gadgets": [
    {
      "name": "formGadget2",
      "title": "Sample form Elements",
      "icon": "FaUser",
      "height": "100%",
      "colSpan": "col-span-4",
      "gadgetType": "form",
      "gadgetOptions": 
      {
        "fields": [
          {
            "type": "textInput",
            "label": "Full Name",
            "name": "fullName",
            "inputType": "text",
            "required": true,
            "minLength": 3,
            "maxLength": 50,
            "width": "w-1/2"
          },
          {
            "type": "textInput",
            "label": "Email",
            "name": "email",
            "inputType": "email",
            "required": true,
            "width": "w-1/2"
          },
          {
            "type": "maskInput",
            "label": "Phone Number",
            "name": "phone",
            "mask": "(###) ###-####",
            "required": true,
            "width": "w-1/3"
          },
          {
            "type": "comboBox",
            "label": "Country",
            "name": "country",
            "options": [
              { "label": "United States", "value": "US" },
              { "label": "Canada", "value": "CA" },
              { "label": "United Kingdom", "value": "UK" }
            ],
            "required": true,
            "width": "w-1/2"
          },
          {
            "type": "numeric",
            "label": "Age",
            "name": "age",
            "min": 0,
            "max": 120,
            "suffix": "years",
            "required": true,
            "width": "w-1/2"
          },
          {
            "type": "dateTimeInput",
            "label": "Appointment Date",
            "name": "appointmentDate",
            "required": true,
            "width": "w-1/2"
          },
          {
            "type": "radioGroup",
            "label": "Gender",
            "name": "gender",
            "options": [
              { "label": "Male", "value": "M" },
              { "label": "Female", "value": "F" },
              { "label": "Other", "value": "O" }
            ],
            "required": true,
            "width": "w-full"
          },
          {
            "type": "rating",
            "label": "Service Rating",
            "name": "serviceRating",
            "value": 3,
            "max": 5,
            "width": "w-1/3"
          },
          {
            "type": "switch",
            "label": "Subscribe to Newsletter",
            "name": "subscribe",
            "checked": false,
            "width": "w-full"
          },
          {
            "type": "textArea",
            "label": "Comments",
            "name": "comments",
            "required": false,
            "minLength": 10,
            "maxLength": 300,
            "width": "w-full"
          }
        ]
      }
      ,
      "footer": "Please complete the form"
    },


    {
      "name": "gridGadget1",
      "title": "User Data",
      "icon": "FaTable",
      "height": "100%",
      "colSpan": "col-span-8",
      "header": "User Information",
      "gadgetType": "grid",
      "gadgetOptions": {
        "columns": [
          { "header": "Name", "field": "name", "type": "text" },
          { "header": "Age", "field": "age", "type": "number" },
          { "header": "Email", "field": "email", "type": "text" }
        ],
        "data": [
          { "name": "John Doe", "age": 30, "email": "john@example.com" }
         
        ]
      },
      "footer": "Last updated 1 day ago"
    },
    {
      "name": "areaChart",
      "title": "Area Chart",
      "icon": "FaChartArea",
      "height": "100%",
      "colSpan": "col-span-4",
      "header": "Area Chart Example",
      "gadgetType": "chart",
